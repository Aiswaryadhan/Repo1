             <script type="text/javascript">
            // removed continueButton if use template on the myforms(creation form iframe)
            if(parent.window.location.pathname.split( '/' )[1] == "myforms" && window.user.accountType.toLowerCase() === 'guest') {
                parent.document.querySelector('#templateSelect').classList.add('hide');
            }

            window.setCustomFunction_signup = true;
            function customFunction_signup() {
                var clonedId = document.querySelector('.js-clicked-signup').value;

                var iFrame = document.querySelector('#iframe-signup');
                var innerDoc = iFrame.contentDocument || iFrame.contentWindow.document;
                if(innerDoc.querySelector('#suUsername').value != '') {
                    window.signupComplete = true;
                    if( parent.window.location.pathname.split( '/' )[1] == "myforms" ){
                        setTimeout( function() {
                            parent.document.querySelector('#templateSelect').click();
                        }, 500);
                    }
                  }
                document.querySelector(".js-"+clonedId).click();
            }

            window.setCustomFunction_login = true;
            function customFunction_login() {
                var clonedId = '';
                var useTemplateButton = '';
                if(parent.window.location.pathname.split( '/' )[1] == "myforms") {
                    clonedId = document.querySelector('.picker-selected').id;
                    useTemplateButton = parent.document.querySelector('#templateSelect');
                } else {
                    clonedId = document.querySelector('.js-clicked-signup').value;
                    useTemplateButton = document.querySelector(".js-"+clonedId);
                }

                var req = new XMLHttpRequest();
                req.open('POST', '/server.php', true);
                req.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
                req.send('action=getLoggedInUser&includeUsage=0');
                req.onreadystatechange = function() {
                    if (this.readyState === 4 && this.status === 200) {
                        var userDetails = JSON.parse(this.responseText);
                        //console.log('userDetails.user.username', userDetails.user.username);
                        if(userDetails.user.username != undefined) {
                            document.querySelector(".js-"+clonedId).click();
                            // console.log('document.querySelector(".js-"+clonedId)', document.querySelector(".js-"+clonedId));
                            useTemplateButton.click();
                        }
                    }
                };
                // console.log('dataLayer', dataLayer);
            }

            function openSignUp() {
                var username = window.user ? window.user.username : '';
                if(username != '' && window.signupUseTemplate == undefined) {
                    window.signupUseTemplate = true;
                }
                addUser('<span class="green">Sign up</span> to use template!');
            }
        </script>
