        <script src="js/lib/resizelistener.js?ver=3.3.8183" type="text/javascript"></script>
        <script>
            (function ($) {
                if (typeof $ === 'function') {
                    $(document).ready(function (e) {
                        var jfHeader   = document.querySelector('.jfHeader'),
                            sLine      = document.querySelector('#searchLine');
                        window.sline   = sLine;
                        var myResizeFn = function (e, x) {
                            var h           = window.getComputedStyle(jfHeader).height;
                            sLine.style.top = h;
                        };
                        myResizeFn();
                        addResizeListener(jfHeader, myResizeFn);
                    });
                }
            })(jQuery);
        </script>
